{
  "expo": {
    "name": "hkControl",
    "slug": "hkControl",
    "owner": "iaguitow",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/second_icon_png.png",
    "scheme": "iowl",
    "splash": {
      "image": "./assets/main_logo_png.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "build": {
      "preview": {
        "android": {
          "buildType": "apk"
        }
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/second_icon_png.png",
          "color": "#ffffff"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/second_icon_png.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Colab.notes",
      "iosDisplayInForeground": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "host.exp.exponent",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/second_icon_png.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "iowl.app.android"
    },
    "web": {
      "favicon": "./assets/second_icon_png.png"
    },
    "extra": {
      "eas": {
        "projectId": "0ea00c36-58f0-4763-ab6c-21310437666a"
      }
    }
  }
}
